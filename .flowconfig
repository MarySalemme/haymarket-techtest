[ignore]
.*/node_modules/stylelint/.*
.*/**/*test.js
.*/.history
.*/**/utils_tests/**/*.js
[include]
.*/src/.*
[libs]
[options]
module.name_mapper='.*\.scss$' -> '<PROJECT_ROOT>/flow/CSSModuleStub.js'
module.name_mapper='.*\.css$' -> '<PROJECT_ROOT>/flow/CSSModuleStub.js'
module.name_mapper='^\(.*\)' -> '<PROJECT_ROOT>/src/\1'
module.name_mapper='^\(.*\)' -> '<PROJECT_ROOT>/src/actions/\1'
module.name_mapper='^\(.*\)' -> '<PROJECT_ROOT>/src/components/\1'
module.name_mapper='^\(.*\)' -> '<PROJECT_ROOT>/src/\1'
module.name_mapper='^\(.*\)$' -> '<PROJECT_ROOT>/src/proptypes/\1/\1'
module.name_mapper='^presentational\/\(.*\)$' -> '<PROJECT_ROOT>/src/components/presentational/\1'